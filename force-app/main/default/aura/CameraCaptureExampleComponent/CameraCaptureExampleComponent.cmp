<aura:component implements="flexipage:availableForAllPageTypes">
    <aura:attribute name="vfHost" type="String" 
        default="momentum-saas-5317-dev-ed--c.cs42.visual.force.com"/>
    <aura:attribute name="imageData" type="String" />

    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <aura:handler name="render" value="{!this}" action="{!c.doRender}"/>

    <iframe aura:id="vfFrame" class="captureFrame" onload="{!c.onFrameLoaded}" allow="geolocation; microphone; camera; midi; encrypted-media;" src="{!'https://' + v.vfHost + '/apex/CameraCaptureExamplePage'}" />
</aura:component>
